package beans;

@Component
public class Car {
    private Engine engine;

    public Car() {
        System.out.println("ğŸš— Car instance created!");
    }

    @Autowired
    public void setEngine(Engine engine) {
        this.engine = engine;
        System.out.println("ğŸ”§ Engine injected via Setter!");
    }

    public void drive() {
        engine.start();
        System.out.println("ğŸš˜ Car is driving...");
    }
}

